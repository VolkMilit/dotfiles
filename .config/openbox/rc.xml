<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc" xmlns:xi="http://www.w3.org/2001/XInclude">
  <resistance>
    <strength>10</strength>
    <screen_edge_strength>20</screen_edge_strength>
  </resistance>
  <focus>
    <focusNew>yes</focusNew>
    <followMouse>no</followMouse>
    <focusLast>yes</focusLast>
    <underMouse>no</underMouse>
    <focusDelay>200</focusDelay>
    <raiseOnFocus>no</raiseOnFocus>
  </focus>
  <placement>
    <policy>Smart</policy>
    <center>yes</center>
    <monitor>Primary</monitor>
    <primaryMonitor>Active</primaryMonitor>
  </placement>
  <theme>
    <name>Numix</name>
    <titleLayout>NLIMC</titleLayout>
    <keepBorder>yes</keepBorder>
    <animateIconify>yes</animateIconify>
    <font place="ActiveWindow">
      <name>Sans</name>
      <size>8</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="InactiveWindow">
      <name>Sans</name>
      <size>8</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="MenuHeader">
      <name>Dejavu-sans book</name>
      <size>9</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="MenuItem">
      <name>Dejavu-sans book</name>
      <size>10</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="ActiveOnScreenDisplay">
      <name>Dejavu-sans book</name>
      <size>9</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
    <font place="InactiveOnScreenDisplay">
      <name>Dejavu-sans book</name>
      <size>9</size>
      <weight>Normal</weight>
      <slant>Normal</slant>
    </font>
  </theme>
  <desktops>
    <number>5</number>
    <firstdesk>3</firstdesk>
    <names>
      <name>1</name>
      <name>2</name>
      <name>3</name>
      <name>4</name>
      <name>5</name>
    </names>
    <popupTime>0</popupTime>
  </desktops>
  <resize>
    <drawContents>yes</drawContents>
    <popupShow>NonPixel</popupShow>
    <popupPosition>Center</popupPosition>
    <popupFixedPosition>
      <x>10</x>
      <y>10</y>
    </popupFixedPosition>
  </resize>
  <margins>
    <top>0</top>
    <bottom>0</bottom>
    <left>0</left>
    <right>0</right>
  </margins>
  <dock>
    <position>TopLeft</position>
    <floatingX>0</floatingX>
    <floatingY>0</floatingY>
    <noStrut>yes</noStrut>
    <stacking>Normal</stacking>
    <direction>Vertical</direction>
    <autoHide>no</autoHide>
    <hideDelay>300</hideDelay>
    <showDelay>300</showDelay>
    <moveButton>Middle</moveButton>
  </dock>
  <keyboard>
    <!-- wm -->
    <chainQuitKey>C-g</chainQuitKey>
    <keybind key="W-d">
      <action name="ToggleShowDesktop"/>
    </keybind>
    <keybind key="A-F4">
      <action name="Close"/>
    </keybind>
    <keybind key="A-Tab">
      <action name="NextWindow">
        <dialog>icons</dialog>
      </action>
    </keybind>
    <keybind key="A-space">
      <action name="ShowMenu">
        <menu>client-menu</menu>
      </action>
    </keybind>
    <keybind key="C-A-KP_5">
      <action name="ToggleMaximizeFull"/>
    </keybind>
    <keybind key="C-A-KP_0">
      <action name="MoveResizeTo">
        <x>center</x>
        <y>center</y>
      </action>
    </keybind>
    <!-- shortcuts -->
    <keybind key="Print">
      <action name="Execute">
        <command>~/scripts/scrotctl</command>
      </action>
    </keybind>
    <keybind key="A-Print">
      <action name="Execute">
        <command>~/scripts/scrotctl -u</command>
      </action>
    </keybind>
    <keybind key="C-A-T">
      <action name="Execute">
        <command>lilyterm</command>
      </action>
    </keybind>
    <keybind key="W-E">
      <action name="Execute">
        <command>spacefm</command>
      </action>
    </keybind>
    <keybind key="W-S">
      <action name="Execute">
        <command>leafpad</command>
      </action>
    </keybind>
    <keybind key="W-G">
      <action name="Execute">
        <command>~/scripts/googleforme</command>
      </action>
    </keybind>
    <keybind key="W-F">
      <action name="Execute">
        <command>~/scripts/openinwww</command>
      </action>
    </keybind>
    <keybind key="W-T">
      <action name="Execute">
        <command>~/scripts/translateforme</command>
      </action>
    </keybind>
    <keybind key="C-A-C">
      <action name="Execute">
        <command>qalculate-gtk</command>
      </action>
    </keybind>
    <keybind key="A-F2">
      <action name="Execute">
        <command>~/scripts/run</command>
