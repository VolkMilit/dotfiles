        <command>~/scripts/stopyt</command>
      </action>
    </keybind>
    <keybind key="XF86HomePage">
      <action name="Execute">
        <command>x-www-browser</command>
      </action>
    </keybind>
    <keybind key="XF86Mail">
      <action name="Execute">
        <command>icedove</command>
      </action>
    </keybind>
    <!-- Desktops -->
    <keybind key="W-F1">
