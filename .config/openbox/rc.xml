    <keybind key="A-F9">
      <action name="Execute">
        <command>~/scripts/xcompmgr</command>
      </action>
    </keybind>
    <keybind key="C-S-Escape">
      <action name="Execute">
        <command>lilyterm -g 140x44 -T "htop" -e htop</command>
      </action>
    </keybind>
    <keybind key="C-A-Delete">
      <action name="Execute">
        <command>lilyterm -g 140x44 -T "htop" -e htop</command>
      </action>
    </keybind>
    <!-- system sortcuts -->
    <keybind key="W-l">
      <action name="Execute">
        <command>~/scripts/lock/lock</command>
      </action>
    </keybind>
    <keybind key="W-Escape">
      <action name="ShowMenu">
        <menu>root-menu</menu>
      </action>
    </keybind>
    <!-- Multimedia keyboard -->
    <keybind key="XF86AudioRaiseVolume">
      <action name="Execute">
        <command>~/scripts/volume 5%+</command>
      </action>
    </keybind>
    <keybind key="XF86AudioLowerVolume">
      <action name="Execute">
        <command>~/scripts/volume 5%-</command>
      </action>
    </keybind>
    <keybind key="XF86AudioMute">
      <action name="Execute">
        <command>~/scripts/volume toggle</command>
      </action>
    </keybind>
    <keybind key="XF86AudioNext">
      <action name="Execute">
        <command>deadbeef --next</command>
      </action>
    </keybind>
    <keybind key="XF86AudioPrev">
      <action name="Execute">
        <command>deadbeef --prev</command>
      </action>
    </keybind>
    <keybind key="XF86AudioPlay">
